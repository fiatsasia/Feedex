{
	"name": "Binance",
	"type": "websocket",
	"endpoint": "wss://testnet-dex.binance.org/api/ws",
	"subscribe": {
		"method": "subscribe",
		"topic": "<topic>",
		"symbols": ["<symbol>"]
	},
	"unsubscribe": {
		"method": "unsubscribe",
		"topic": "<topic>",
		"symbols": ["<symbol>"]
	},
	"subscribeArgs": {
		"filters": [
			{ "path": "stream", "value": "<topic>" }
		],
		"path": "data"
	},
	"close": {
		"method": "close"
	}
}
