{
	"name": "Binance",
	"type": "websocket",
	"endpoint": "wss://stream.binance.com:9443/stream",
	"subscribe": {
		"method": "SUBSCRIBE",
		"params": ["<symbol>@<event>"],
		"id": "%id%"
	},
	"unsubscribe": {
		"method": "UNSUBSCRIBE",
		"params": ["<symbol>@<event>"],
		"id": "%id%"
	},
	"subscribeArgs": {
		"filters": [
			{ "stream": "<symbol>@<event>" }
		],
		"path": "data"
	},
	"close": {
		"method": "close"
	}
}
