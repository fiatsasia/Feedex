{
	"name": "Binance",
	"type": "websocket",
	"endpoint": "wss://dex.binance.org/api/ws",
	"defaultKeepAliveInterval": "00:00:30",
	"subscribe": {
		"method": "SUBSCRIBE",
		"params": [ "<market>@<channel>" ],
		"id": "<id>"
	},
	"unsubscribe": {
		"method": "UNSUBSCRIBE",
		"params": [ "<market>@<channel>" ].
		"id": "<id>"
	}
}
