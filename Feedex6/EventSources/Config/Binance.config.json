{
	"name": "Binance",
	"type": "websocket",
	"endpoint": "wss://testnet-dex.binance.org/api/ws",
	"pingIntervalMs": 1800000,
	"ping": {
		"method": "keepAlive"
	},
	"subscribe": {
		"method": "subscribe",
		"topic": "<topic>",
		"symbols": ["<symbol>"]
	},
	"unsubscribe": {
		"method": "unsubscribe",
		"topic": "<topic>",
		"symbols": ["<symbol>"]
	},
	"close": {
		"method": "close"
	}
}
