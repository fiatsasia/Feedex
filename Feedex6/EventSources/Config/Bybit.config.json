{
	"type": "websocket",
	"endpoint": "wss://stream-testnet.bybit.com/realtime",
	"pingIntervalMs": 20000,
	"ping": {
		"op": "ping"
	},
	"subscribe": {
		"op": "subscribe",
		"args": ["<topic>.<filter>"]
	},
	"unsubscribe": {
		"op": "unsubscribe",
		"args": ["<topic>.<filter>"]
	},
	"subscribeArgs": {
		"filters": [
			{ "path": "topic", "value": "<topic>.<filter>" }
		]
	}
}
