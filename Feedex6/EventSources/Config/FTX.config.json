{
	"name": "FTX",
	"type": "websocket",
	"endpoint": "wss://ftx.com/ws/",
	"pingIntervalMs": 15000,
	"ping": {
		"op": "ping"
	},
	"subscribe": {
		"op": "subscribe",
		"channel": "<channel>",
		"market": "<market>"
	},
	"unsubscribe": {
		"op": "unsubscribe",
		"channel": "<channel>",
		"market": "<market>"
	}
	"subscribeArgs": {
		"filters": [
			{ "path": "type", "value": "update" },
			{ "path": "channel", "value": "<channel>"},
			{ "path": "market", "value": "<market>"}
		],
		"path": "data"
	},
	"channels": [ "orderbook", "trades", "ticker" ]
}
