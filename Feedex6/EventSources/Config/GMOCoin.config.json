{
	"type": "websocket",
	"endpoint": "wss://api.coin.z.com/ws/public/v1",
	"minimumSendIntervalMs": 1000,
	"subscribe": {
		"command": "subscribe",
		"channel": "<channel>",
		"symbol": "<symbol>"
	},
	"unsubscribe": {
		"command": "unsubscribe",
		"channel": "<channel>",
		"symbol": "<symbol>"
	},
	"subscribeArgs" {
		"filters": [
			{ "path": "channel", "value": "<channel>" },
			{ "path": "symbol", "value": "<symbol>" }
		]
	}
}
