{
	"type": "websocket",
	"endpoint": "wss://api.coin.z.com/ws/public/v1",
	"subscribe": {
		"command": "subscribe",
		"channel": "<channel>",
		"symbol": "<symbol>"
	},
	"unsubscribe": {
		"command": "unsubscribe",
		"channel": "<channel>",
		"symbol": "<symbol>"
	},
	"filters": [
		{ "path": "$.channel", "value": "<channel>" },
		{ "path": "$.symbol", "value": "<symbol>" }
	]
}
